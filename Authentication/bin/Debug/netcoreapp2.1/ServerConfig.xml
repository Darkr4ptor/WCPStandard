<?xml version="1.0" encoding="utf-8"?>
<Config>

  <AuthenticationServer>
    <!--
 Database data contains information required to connect to
  the server database
  -->
    <Database>
    <Host>127.0.0.1</Host>
    <Port>3306</Port>
    <UserName>root</UserName>
    <UserPassword></UserPassword>
    <DatabaseName>masterserver</DatabaseName>
    </Database>
    <Server>
      <MaximumGameServers>10</MaximumGameServers>
      <ServerMode>0</ServerMode>
    </Server>
  </AuthenticationServer>
  
  <GameServer>
    <!-- Game database-->
    <Database>
      <Host>127.0.0.1</Host>
      <Port>3306</Port>
      <UserName>root</UserName>
      <UserPassword></UserPassword>
      <DatabaseName>wres2</DatabaseName>
    </Database>
    <AuthServer>
      <!-- Enter the Authentication server IP. If both are running on the same machine, change it 127.0.0.1
      Key is unused atm.
      -->     
      <IP>127.0.0.1</IP>
      <Key>AlterEmuV5.0.1</Key>  
    </AuthServer>

    <!-- Game Server DATA.-->
    <Server>
      <Name>Spain</Name>
      <IP>127.0.0.1</IP>
      <ServerMode>0</ServerMode>
    </Server>
  </GameServer>
</Config>
